@model JobSeek.Models.JobType
@{
    ViewData["Title"] = "Details";
}
<br />
<h2 class="text-dark">Job Type Details</h2>
<form asp-action=Edit method="post">
    <div class="p-4 rounded border">
        <input type="hidden" asp-for="Id" />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="JobTypes"></label>
            </div>
            <div class="col-5">
                <input asp-for="JobTypes" class="form-control" readonly="readonly">
            </div>
            <span asp-validation-for="JobTypes" class="text-danger"></span>
        </div>
        <div class="form-group">
            <a asp-action="Edit" asp-route-id="@Model.Id" class="btn btn-primary">Edit</a>
            <a asp-action="Index" class="btn btn-success">Back to List</a>
        </div>
    </div>
</form>

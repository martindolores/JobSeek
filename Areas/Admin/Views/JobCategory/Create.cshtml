@model JobSeek.Models.JobCategory
@{
    ViewData["Title"] = "Create";
}
<br />
<h2 class="text-dark">Add New Job Category</h2>
<form asp-action=Create enctype="multipart/form-data" method="post">
    <div class="p-4 rounded border">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group row">
            <div class="col-2">
                <label asp-for="JobCategories"></label>
            </div>
            <div class="col-5">
                <input asp-for="JobCategories" class="form-control">
            </div>
            <span asp-validation-for="JobCategories" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" class="btn btn-primary" value="Save" />
            <a asp-action="Index" class="btn btn-success">Back to List</a>
        </div>
    </div>
</form>
